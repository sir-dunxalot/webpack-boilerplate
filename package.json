{
  "name": "webpack-boilerplate",
  "version": "0.0.0",
  "description": "Quick start config for webpack apps using babel, node-sass, npm scripts, and webpack-dev-server",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --progress --colors",
    "deploy": "surge dist",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "start-for-office-lan": "webpack-dev-server --host 10.1.10.237",
